{
  "$schema": "node_modules/wrangler/config-schema.json",
  "compatibility_date": "2025-01-09",
  "compatibility_flags": ["nodejs_compat"],
  "pages_build_output_dir": "./dist",
  "observability": {
    "enabled": true
  },
  "name": "demo-site",
  "env": {
    "NODE_ENV": "production"
  },
  "vars": {
    "ENVIRONMENT": "production"
  },
  "triggers": {
    "crons": []
  },
  "rules": [
    {
      "type": "ESModule",
      "globs": ["**/*.js"]
    }
  ],
  "build": {
    "command": "npm run build",
    "cwd": ".",
    "watch_dir": "src"
  },
  "site": {
    "bucket": "./dist"
  },
  "routes": [
    {
      "pattern": "/*",
      "zone_name": "demo-site.pages.dev"
    }
  ],
  "headers": [
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
